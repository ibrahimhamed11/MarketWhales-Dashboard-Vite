# Environment Configuration Template
# Copy this file to .env and configure your settings

# API Configuration
# For local development: http://localhost:4000
# For production: https://market-whales.onrender.com
VITE_API_URL=https://market-whales.onrender.com

# Mux Configuration (required for video streaming)
# Get your ENV_KEY from https://dashboard.mux.com/environments
VITE_MUX_ENV_KEY=your_mux_environment_key_here

# Optional: Custom beacon collection domain for Mux Data
# VITE_MUX_BEACON_DOMAIN=your-custom-domain.com

# Development settings
NODE_ENV=development
VITE_DEBUG_MUX=true
VITE_DEV_MODE=false
VITE_DEBUG_API=false
